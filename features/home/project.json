{
  "name": "home",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "features/home/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "outputs": [
        "{projectRoot}/{options.outDir}",
        "{workspaceRoot}/dist/features/home"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outDir": "../../dist/features/home"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "outputs": [
        "{projectRoot}/{options.coverage.reportsDirectory}",
        "{workspaceRoot}/coverage/features/home"
      ],
      "options": {
        "coverage.reportsDirectory": "../../coverage/features/home"
      }
    },
    "component-test": {
      "options": {
        "config-file": "./cypress.config.ts"
      }
    }
  }
}

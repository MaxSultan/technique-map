{
  "name": "map-items",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "features/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "outputs": [
        "{projectRoot}/{options.outDir}",
        "{workspaceRoot}/features/dist/map-items"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outDir": "../../dist/features"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "outputs": [
        "{projectRoot}/{options.coverage.reportsDirectory}",
        "{workspaceRoot}/features/coverage/map-items"
      ],
      "options": {
        "passWithNoTests": true,
        "coverage.reportsDirectory": "../../coverage/features"
      }
    }
  }
}

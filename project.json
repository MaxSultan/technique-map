{
  "name": "technique-map",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "./src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "outputs": [
        "{projectRoot}/{options.outDir}",
        "{projectRoot}/dist/technique-map"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outDir": "./dist/technique-map"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "defaultConfiguration": "development",
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "preview": {
      "defaultConfiguration": "development",
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "test": {
      "outputs": [
        "{projectRoot}/{options.coverage.reportsDirectory}",
        "{projectRoot}/coverage/technique-map"
      ],
      "options": {
        "coverage.reportsDirectory": "./coverage/technique-map"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "technique-map:build"
      }
    }
  }
}
